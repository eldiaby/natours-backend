doctype html 
html 
  head 
    meta(charset='UTF-8')
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel='stylesheet' href='https://fonts.googleapis.com/css?family=Lato:300,300i,700')
    link(rel='stylesheet', href='css/style.css')
    link(rel='shortcut icon' type='image/png' href='img/favicon.png') 
    title Natours | Exciting tours for adventurous people

  body 
    // Start of header
    header.header 
      nav.nav.nav--tours 
        a.nav__el(href='#')
      .header__logo 
        img(src='./img/logo-white.png', alt='Natours logo' title='Natours logo')
      nav.nav.nav--user 
        //- a.nav__el(href='#') My bookings 
        //- a.nav__el(href='#') 
        //-   img(src='./img/user.jpg', alt='User photo' title='User photo')
        //-   span Jones 
        button.nav__el Log in
        button.nav__el.nav__el--cta Sign up

    // Start of overview
    section.overview 
      h1= tour

    // Start of ooter
    footer.footer 
        .footer__logo 
          img(src='./img/logo-green.png', alt='Natours logo' title='Natours logo')
        ul.footer__nav 
          li: a(href='#') About us  
          li: a(href='#') Download apps
          li: a(href='#') Become a guide 
          li: a(href='#') Careers
          li: a(href='#') Contact
